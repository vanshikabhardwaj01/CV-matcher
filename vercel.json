{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/match", "dest": "api/index.py" },
    { "src": "/download_report", "dest": "api/index.py" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}